token {
   # AUTH_SERVICE_TOKEN_TIMEOUT
   # Timeout in seconds:
   timeout = 3600 #1h
}

db {
  # AUTH_SERVICE_DB_HOST
  host = "database"
  # AUTH_SERVICE_DB_DATABASE
  database = "auth-service"
  # AUTH_SERVICE_DB_USER
  user = "auth-service"
  # AUTH_SERVICE_DB_PASSWORD
  password = "password" #required
}

otp {
  # AUTH_SERVICE_OTP_LENGTH
  # generated One Time Password length
  otp.length = 8
  # AUTH_SERVICE_OTP_TIMEOUT
  # One Time Password timeout in seconds
  otp.timeout = 300 # 5 minutes
}